#Write a program that finds the maximum and minimum values in a Numpy array.
import numpy as np

def find_max_min(arr):
    max_val = np.max(arr)
    min_val = np.min(arr)

    print(f"Array: {arr}")
    print(f"Maximum value: {max_val}")
    print(f"Minimum value: {min_val}")

array = np.array([15, 2, 89, 34, 6, 77])
find_max_min(array)
